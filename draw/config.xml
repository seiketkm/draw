<OpenNI>

  <Licenses></Licenses>

  <Log writeToConsole="true" writeToFile="false">

    <!-- 0 - Verbose, 1 - Info, 2 - Warning, 3 - Error (default) -->

    <LogLevel value="0"/>

    <Masks>

      <Mask name="ALL" on="false"/>

    </Masks>

    <Dumps>

    </Dumps>

  </Log>

  <ProductionNodes>

    <Node type="Depth" name="Depth1" stopOnError="false">

      <Configuration>

        <Mirror on="true"/>
        <MapOutputMode xRes="640" yRes="480" FPS="30"/>
      </Configuration>

    </Node>

    <Node type="Image" name="Image1" stopOnError="false">

      <Configuration>

        <Mirror on="true"/>
        <MapOutputMode xRes="640" yRes="480" FPS="30"/>
      </Configuration>

    </Node>

    <Node type="User" name="User1" />



  </ProductionNodes>

</OpenNI>